{
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "commonjs": true
  },
  "parserOptions": {
    "parser": "babel-eslint",
    "ecmaVersion": 8,
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "extends": [
    "airbnb-base",
    "plugin:vue/recommended"
  ],
  "rules": {
    "vue/no-v-html": "off",
    "no-restricted-syntax": 0,
    "no-param-reassign": 0,
    "import/no-unresolved": 0,
    "import/extensions": 0,
    "import/order": 0,
    "import/no-extraneous-dependencies": 0,
    "no-console": 0,
    "no-nested-ternary": 0,
    "callback-return": 0,
    "import/prefer-default-export": 0,
    "global-require": 0,
    "consistent-return": 0,
    "linebreak-style": 0,
    "no-return-assign": 0,
    "no-shadow": 0,
    "no-bitwise": 0,
    "vue/comment-directive": 0,
    "vue/attribute-hyphenation": 0,
    "vue/html-quotes": 0,
    "vue/no-unused-components": "warn",
    "vue/html-self-closing": [
      "error",
      {
        "html": {
          "void": "never",
          "normal": "never",
          "component": "always"
        },
        "svg": "always",
        "math": "always"
      }
    ],
    "vue/attributes-order": [
      2,
      {
        "order": [
          "LIST_RENDERING",
          "CONDITIONALS",
          "RENDER_MODIFIERS",
          "GLOBAL",
          "UNIQUE",
          "BINDING",
          "OTHER_ATTR",
          "EVENTS",
          "CONTENT",
          "DEFINITION"
        ]
      }
    ],
    "vue/require-default-prop": 0,
    "vue/max-attributes-per-line": [
      2,
      {
        "singleline": 3,
        "multiline": {
          "max": 3,
          "allowFirstLine": true
        }
      }
    ],
    "vue/prop-name-casing": 0,
    "vue/html-indent": [
      "error",
      4,
      {
        "attribute": 1,
        "closeBracket": 0,
        "alignAttributesVertically": true,
        "ignores": []
      }
    ],
    "indent": [
      "error",
      4
    ],
    "max-len": [
      0,
      {
        "tabWidth": 4
      }
    ],
    "no-self-assign": [
      "error",
      {
        "props": false
      }
    ],
    "no-redeclare": "error",
    "no-lonely-if": "error",
    "no-multiple-empty-lines": [
      1,
      {
        "max": 1
      }
    ],
    "new-parens": "error",
    "block-spacing": "error",
    "no-const-assign": "error",
    "no-this-before-super": "warn",
    "no-undef": "error",
    "no-unused-vars": "warn",
    "constructor-super": "warn",
    "valid-typeof": "warn",
    "semi": [
      "error",
      "never"
    ],
    "no-extra-semi": "error",
    "no-constant-condition": "error",
    "no-unreachable": "error",
    "no-unneeded-ternary": "error",
    "no-dupe-keys": "error",
    "no-dupe-class-members": "error",
    "key-spacing": [
      1,
      {
        "singleLine": {
          "beforeColon": false,
          "afterColon": true
        },
        "multiLine": {
          "align": "colon"
        }
      }
    ]
  }
}
